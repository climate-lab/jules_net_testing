@article{Greve1998,
  title = {Performance {{Aspirations}}, and {{Risky Organizational Change}}},
  author = {Greve, Henrich R},
  date = {1998},
  journaltitle = {Administrative Science Quarterly},
  shortjournal = {ASQ},
  volume = {43},
  number = {1},
  eprint = {751334},
