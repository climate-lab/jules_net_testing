@article{Caldwell2011,
  title = {{{HR}} Directors in {{UK}} Boardrooms: {{A}} Search for Strategic Influence or Symbolic Capital?},
  shorttitle = {{{HR}} Directors in {{UK}} Boardrooms},
  author = {Caldwell, Raymond},
  date = {2011-01-04},
  journaltitle = {Employee Relations},
  volume = {33},
