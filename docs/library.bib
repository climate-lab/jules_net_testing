@article{Chia1995,
  title = {From {{Modern}} to {{Postmodern Organizational Analysis}}},
  author = {Chia, Robert},
  date = {1995-07},
  journaltitle = {Organization Studies},
  shortjournal = {Org Studies},
  volume = {16},
