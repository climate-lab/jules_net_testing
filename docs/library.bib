@article{Lecuyer2018,
  title = {Building on Common Ground to Address Biodiversity Conflicts and Foster Collaboration in Environmental Management},
  author = {Lecuyer, Lou and White, Rehema M. and Schmook, Birgit and Calmé, Sophie},
  date = {2018-08},
  journaltitle = {Journal of Environmental Management},
